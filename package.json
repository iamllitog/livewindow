    "axios": "^0.18.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "^1.4.3",
    "debug": "~2.6.3",
    "echarts": "^4.1.0",
    "ejs": "~2.5.6",
    "express": "^4.15.3",
    "morgan": "~1.8.1",
    "mysql": "^2.14.0",
    "node-schedule": "^1.2.4",
    "nuxt": "^1.4.0",
    "request": "^2.88.0",
    "serve-favicon": "~2.4.2",
    "source-map-support": "^0.4.15",
    "superagent": "^3.5.2",
    "unirest": "^0.5.1",
    "vue-lazyload": "^1.2.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
