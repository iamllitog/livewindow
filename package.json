{
  "name": "livewindow",
  "version": "0.0.8",
  "private": true,
  "scripts": {
    "dev": "backpack dev",
    "build": "cross-env NODE_ENV=production nuxt build && backpack build",
    "start": "cross-env NODE_ENV=production node build/main.js",
    "prod_start": "npm run build && pm2 start PM2.json",
    "prod_restart": "npm run build && pm2 restart livewindow",
